[run]
source = manifestapp
omit =
    tests
    venv

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError

[html]
directory = coverage_html_report

[xml]
output = coverage.xml

[json]
output = coverage.json